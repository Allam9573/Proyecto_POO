<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>ClienteController</servlet-name>
  	<display-name>ClienteController</display-name>
  	<description></description>
  	<servlet-class>controllers.ClienteController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ProductoController</servlet-name>
  	<display-name>ProductoController</display-name>
  	<description></description>
  	<servlet-class>controllers.ProductoController</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>ClienteController</servlet-name>
  	<url-pattern>/ClienteController</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ProductoController</servlet-name>
  	<url-pattern>/ProductoController</url-pattern>
  </servlet-mapping>
</web-app>
